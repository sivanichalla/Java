import java.io.*;
import java.util.*;
class Bike{
    void run(){
        System.out.println("Bike is running");
    }
}
public class Splendor extends Bike{
    void run(){
        System.out.println("Splendor is running with 60kmph");
    }
    public static void main(String args[]){
        Splendor s = new Splendor();
        s.run();
        Bike b1 = new Bike();
        b1.run();
        Bike b2 = new Splendor();
        b2.run();
    }
}
